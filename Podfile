  pod 'TZImagePickerController', '~>2.1'
  # 布局
  pod 'Masonry','~>1.1'
  pod 'SDAutoLayout','~>2.2'
  
  # HUD
  pod 'MBProgressHUD', '~> 0.9'
  
  # 弹出框
  pod 'ActionSheetPicker-3.0','~>2.3'
  
  # UI 组件
  pod 'SDCycleScrollView','~>1.0' # 轮播图
  pod 'HMSegmentedControl','~>1.5' # 分栏
  pod 'IQKeyboardManager','~>6.0' # 键盘适配
  
  # 加密
  pod 'CocoaSecurity','~>1.2'
  
  # 缓存
